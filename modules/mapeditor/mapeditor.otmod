Module
  name: mapeditor
  description: Initialize the mapeditor and setups its main window
  author: fallen, edubart
  reloadable: false

  load-later:
    - client_skins
    - client_topmenu
    - client_terminal
    - client_modulemanager
    - mapeditor_startup
    - mapeditor_interface
    - mapeditor_minimap

  @onLoad: |
    dofile 'mapeditor'
    MapEditor.init()

  @onUnload: |
    MapEditor.terminate()
